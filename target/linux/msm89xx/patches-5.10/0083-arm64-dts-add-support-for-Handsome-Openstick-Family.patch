+			linux,default-trigger = "heartbeat";
+			default-state = "on";
+		};
+
+	};
+};
+
+&pronto {
+	status = "okay";
+
+	iris {
+		compatible = "qcom,wcn3660b";
+	};
+};
+
+&msmgpio {
+	msmgpio_leds: msmgpio-leds {
+		pins = "gpio28","gpio10","gpio9";
+		function = "gpio";
+
+		bias-disabled;
+	};
+
+	gpio_keys_default: gpio_keys_default {
